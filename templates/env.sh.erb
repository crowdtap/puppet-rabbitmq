# Let the env know where rabbitmq runs.

export BOXEN_RABBITMQ_PORT=<%= scope.lookupvar('rabbitmq::config::port') %>
export BOXEN_RABBITMQ_URL="rabbitmq://guest:guest@localhost:$BOXEN_RABBITMQ_PORT/"
export BOXEN_RABBITMQ_MGMT_PORT=<%= scope.lookupvar('rabbitmq::config::mgmt_port') %>
export BOXEN_RABBITMQ_MGMT_URL="rabbitmq://guest:guest@localhost:$BOXEN_RABBITMQ_MGMT_PORT/"
